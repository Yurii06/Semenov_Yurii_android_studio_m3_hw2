<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.FourthFragment">

    <TextView
        android:id="@+id/text_view_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ваши данные :"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_view_two"
        app:layout_constraintTop_toBottomOf="@id/text_view_one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_view_three"
        app:layout_constraintTop_toBottomOf="@id/text_view_two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_view_four"
        app:layout_constraintTop_toBottomOf="@id/text_view_three"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_view_five"
        app:layout_constraintTop_toBottomOf="@id/text_view_four"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_view_six"
        app:layout_constraintTop_toBottomOf="@id/text_view_five"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_view_seven"
        app:layout_constraintTop_toBottomOf="@id/text_view_six"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>